<%= form_with(model: @blog, local: true) do |form| %>
    <div>
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <div>
    <%= form.label :image %>
    <%= form.file_field :image %>
    <%= form.hidden_field :image_cache%>
  </div>
  <%= form.submit %>
<% end %>